{
	"name": "ninjaparade/slack",
	"type": "platform-extension",
	"description": "A Platform 3 Slack extension",
	"authors": [
		{
			"name": "Ninjaparade Digital Inc.",
			"email": "yj@ninjaparade.ca"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"illuminate/support": "4.2.*",
		"platform/foundation": "2.0.*",
		"guzzlehttp/guzzle": "~5.0"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Ninjaparade\\Slack\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
